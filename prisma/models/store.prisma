model Store {
  id             String            @id @default(cuid())
  name           String
  categories     Category[]
  products       Product[]
  orders         Order[]
  stock_receipts StockReceipt[]
  employees      Employee[]
  roles          Role[]
  events         Event[]
  broadcasts     Broadcast[]
  applications   LineApplication[]
  consignments   Consignment[]
  //         Add ress
  address        String?
  district       String?
  area           String?
  province       String?
  postalcode     String?
  lineBotConfig  LineBotConfig?
  api_key        String?           @unique

  @@map("stores")
}
