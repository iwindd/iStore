model Store {
  id             String            @id @default(cuid())
  slug           String            @db.VarChar(50) @unique
  name           String            @db.VarChar(100)
  categories     Category[]
  products       Product[]
  orders         Order[]
  stock_receipts StockReceipt[]
  employees      Employee[]
  store_roles    StoreRole[]
  events         Event[]
  broadcasts     Broadcast[]
  applications   LineApplication[]
  consignments   Consignment[]
  addresses      StoreAddress[]

  @@map("stores")
}
