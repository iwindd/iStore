model Store {
  id         String       @id @default(cuid())
  name       String
  categories Category[]
  product    Product[]
  orders     Order[]
  borrows    Borrow[]
  stocks     Stock[]
  users      UserStore[]
  roles      Role[]
  events     Event[]
  //         Address
  line_token String?
  address    String?
  district   String?
  area       String?
  province   String?
  postalcode    String?
  lineBotConfig LineBotConfig?

  @@map("stores")
}